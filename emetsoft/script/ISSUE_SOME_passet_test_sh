
ISSUER=passet_issuer 
SELECTOR=passet_selector 
TRANSFER=passet_transfer 

cd /home/jose/tmp
rm -rf passet_test
mkdir passet_test

cd ./passet_test

DOM_1=dom01.com:9101
DOM_2=dom02.net:9102
DOM_3=dom03.gov:9103
DOM_4=dom04.org:9104

$ISSUER -k "usu1" -c -snm "nm_usu1" -sid "id_001" -scc "CO" -sdn "$DOM_1"
$ISSUER -k "usu2" -c -snm "nm_usu2" -sid "id_002" -scc "CO" -sdn "$DOM_2"
$ISSUER -k "usu3" -c -snm "nm_usu3" -sid "id_003" -scc "CO" -sdn "$DOM_3"
$ISSUER -k "usu4" -c -snm "nm_usu4" -sid "id_004" -scc "CO" -sdn "$DOM_4"

$ISSUER -s -n 1 -m COP 
 
$ISSUER -k "usu1" -ISSUE 10
$ISSUER -k "usu2" -ISSUE 10
$ISSUER -k "usu3" -ISSUE 10

$ISSUER -s -n 1z -m COP 

$ISSUER -k "usu1" -ISSUE 10
$ISSUER -k "usu2" -ISSUE 10
$ISSUER -k "usu3" -ISSUE 10

$ISSUER -s -n 5z -m COP 

$ISSUER -k "usu1" -ISSUE 10
$ISSUER -k "usu2" -ISSUE 10
$ISSUER -k "usu3" -ISSUE 10

