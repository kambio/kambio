
KILL_passet_test_sh

CURR=COP
ISSUER=passet_issuer 
SELECTOR=passet_choose 
TRANSFER=passet_transfer 
PROTO=-mudp
# PROTO=-tcp

cd /home/jose/tmp
rm -rf passet_test
mkdir passet_test

cd ./passet_test

DOM_1=dom01.com:9101:1000
DOM_2=dom02.net:9102:1000
DOM_3=dom03.gov:9103:1000
DOM_4=dom04.org:9104:1000

TDOM_1=dom01.com
TDOM_2=dom02.net
TDOM_3=dom03.gov
TDOM_4=dom04.org

$ISSUER -k "usu1" $PROTO -c -snm "nm_usu1" -sid "id_001" -scc "CO" -sdn "$DOM_1" -glid
$ISSUER -k "usu2" $PROTO -c -snm "nm_usu2" -sid "id_002" -scc "CO" -sdn "$DOM_2" -glid
$ISSUER -k "usu3" $PROTO -c -snm "nm_usu3" -sid "id_003" -scc "CO" -sdn "$DOM_3" -glid
$ISSUER -k "usu4" $PROTO -c -snm "nm_usu4" -sid "id_004" -scc "CO" -sdn "$DOM_4" -glid

$ISSUER -k "usu1" $PROTO -prt_glid
$ISSUER -k "usu2" $PROTO -prt_glid
$ISSUER -k "usu3" $PROTO -prt_glid
$ISSUER -k "usu4" $PROTO -prt_glid


