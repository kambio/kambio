
KILL_passet_test_sh

CURR=COP
ISSUER=passet_issuer 
SELECTOR=passet_choose 
TRANSFER=passet_transfer 
PROTO=-mudp
# PROTO=""

cd /home/jose/tmp
rm -rf passet_test
mkdir passet_test

cd ./passet_test

GAM_FF=../gamal_sys_0.dat

PORT_S=12345

$ISSUER -k "supra" $PROTO -c -snm "nm_supra" -sid "id_supra" -scc "CO" -glid -gamal $GAM_FF

GLI_S=`$ISSUER -k "supra" $PROTO -prt_glid`

echo "GS="$GLI_S

$TRANSFER -k "supra" $PROTO -port "$PORT_S" -is_supra &


