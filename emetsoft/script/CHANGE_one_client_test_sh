
ISSUER=passet_issuer 
SELECTOR=passet_choose 
TRANSFER=passet_transfer 
PROTO=-mudp
# PROTO=""

cd /home/jose/tmp/passet_test

DOM_1=dom01.com:9101:1000
DOM_2=dom02.net:9102:1000
DOM_3=dom03.gov:9103:1000
DOM_4=dom04.org:9104:1000

CLI_1=localhost:9201:1000
CLI_2=localhost:9202:1000
CLI_3=localhost:9203:1000
CLI_4=localhost:9204:1000

GLI_1=`$ISSUER -k "usu1" $PROTO -prt_glid`
GLI_2=`$ISSUER -k "usu2" $PROTO -prt_glid`
GLI_3=`$ISSUER -k "usu3" $PROTO -prt_glid`
GLI_4=`$ISSUER -k "usu4" $PROTO -prt_glid`

START_TM=`get_time_sh`

$SELECTOR -k usu1 $PROTO +1 5z3
$TRANSFER -k usu1 $PROTO -send -l "$CLI_1" -r "$DOM_4"
echo ""
echo ""
echo ""
echo "ONE FINISHED ONE FINISHED ONE FINISHED"

$SELECTOR -k usu4 $PROTO -dn "$GLI_1" +1 5z3
$TRANSFER -k usu4 $PROTO -split -l "$CLI_4" -r "$DOM_1"
echo ""
echo ""
echo ""
echo "TWO FINISHED TWO FINISHED TWO FINISHED"

$SELECTOR -k usu4 $PROTO 

echo "TOTAL TIME="
get_time_sh $START_TM

