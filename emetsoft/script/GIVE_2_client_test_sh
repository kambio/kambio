
ISSUER=passet_issuer 
SELECTOR=passet_choose 
TRANSFER=passet_transfer 
PROTO=-mudp
# PROTO=""

cd /home/jose/tmp/passet_test

GLI_1=`$ISSUER -k "usu1" $PROTO -prt_glid`
GLI_2=`$ISSUER -k "usu2" $PROTO -prt_glid`

# DOM_1=dom01.com:9101:7000
# DOM_2=dom02.net:9102:7000

PORT_1=9201
PORT_2=9202

$SELECTOR -k usu1 $PROTO +1 5z3
$TRANSFER -k usu1 $PROTO -send -port "$PORT_1" -r "$GLI_2"

$SELECTOR -k usu2 $PROTO +1 5z3
$TRANSFER -k usu2 $PROTO -send -port "$PORT_2" -r "$GLI_1"

$SELECTOR -k usu2 $PROTO 
$SELECTOR -k usu1 $PROTO 

